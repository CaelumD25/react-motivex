{
  "name": "MotivexReact",
  "version": "1.0.0",
  "scripts": {
    "start": "$env:EXPO_TV = 'true'; npx expo start",
    "android": "$env:EXPO_TV = 'true'; npx expo run:android",
    "ios": "EXPO_TV=1 expo run:ios",
    "web": "$env:EXPO_TV = 'true'; npx expo start --web",
    "prebuild": "$env:EXPO_TV = 'true'; npx expo prebuild --clean"
  },
  "dependencies": {
    "@config-plugins/react-native-ble-plx": "^7.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/progress-bar-android": "^1.0.5",
    "@react-native-community/slider": "4.5.5",
    "@react-navigation/native": "^7.0.13",
    "@react-navigation/native-stack": "^7.1.14",
    "expo": "^52.0.18",
    "expo-av": "~15.0.1",
    "expo-dev-client": "~5.0.6",
    "expo-document-picker": "~13.0.1",
    "expo-file-system": "~18.0.4",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.0",
    "expo-video": "~2.0.2",
    "expo-video-thumbnails": "~9.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "npm:react-native-tvos@~0.76.3-0",
    "react-native-ble-plx": "^3.2.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "~1.11.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@eslint/js": "^9.16.0",
    "@react-native-tvos/config-tv": "^0.1.1",
    "@types/react": "^18.3.12",
    "@types/react-native": "^0.73.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "prettier": "^3.4.2",
    "typescript": "~5.3.3",
    "typescript-eslint": "^8.17.0"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ],
      "android": {
        "permissions": [
          "READ_EXTERNAL_STORAGE"
        ]
      }
    }
  },
  "private": true
}
